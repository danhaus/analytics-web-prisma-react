{
  "name": "@analytics-dashboard-express-react/server",
  "version": "1.0.0",
  "main": "dist/src/index.js",
  "author": "Daniel Hausner",
  "license": "MIT",
  "scripts": {
    "build": "dotenv -e .env -- tsc",
    "start": "dotenv -e .env -- NODE_ENV=production node ./build/index.js",
    "dev": "dotenv -e .env -- ts-node-dev --respawn --transpile-only ./src/index.ts",
    "test": "dotenv -e .env.test -- yarn docker:up && yarn prisma:db:push && jest -i",
    "prisma:format": "npx prisma format",
    "prisma:studio": "npx prisma studio",
    "prisma:db:push": "dotenv -e .env npx prisma db push",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/node": "^15.12.5",
    "prisma": "^2.26.0",
    "ts-node-dev": "^1.1.7",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "@prisma/client": "^2.26.0",
    "cors": "^2.8.5",
    "express": "^4.17.1"
  }
}
